   LETTER = "abcdefghijklmnopqrstuvwxyz" 
   LETTERORDOT = "." LETTER
   LETTERORSLASH = "/" LETTER
   OUTPUT = "part1"
   LINE = INPUT
   LINE = SPAN(LETTER) . PROTO "://" SPAN(LETTERORDOT) . HOST "/" SPAN(LETTERORSLASH) . RES
   OUTPUT = LETTERORSLASH
   OUTPUT = PROTO
   OUTPUT = HOST 
   OUTPUT = RES
END
